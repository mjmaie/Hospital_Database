<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxml.PatientTestsController">
    <columnConstraints>
        <ColumnConstraints maxWidth="121.0" minWidth="-Infinity" prefWidth="100.0" />
        <ColumnConstraints maxWidth="120.0" minWidth="-Infinity" prefWidth="178.0" />
        <ColumnConstraints maxWidth="134.0" minWidth="-Infinity" prefWidth="120.0" />
        <ColumnConstraints maxWidth="120.0" minWidth="-Infinity" prefWidth="120.0" />
        <ColumnConstraints maxWidth="140.0" minWidth="-Infinity" prefWidth="140.0" />
        <ColumnConstraints maxWidth="120.0" minWidth="-Infinity" prefWidth="120.0" />
        <ColumnConstraints maxWidth="12.0" minWidth="0.0" prefWidth="0.0" />
        <ColumnConstraints />
      <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
    </rowConstraints>
    <children>
        <Label alignment="CENTER_RIGHT" prefHeight="27.0" prefWidth="192.0" text="Patient Testing" GridPane.columnIndex="2" GridPane.columnSpan="2">
         <font>
            <Font size="18.0" />
         </font></Label>
      <Label text="Blood Tests:" GridPane.rowIndex="2" />
      <RadioButton mnemonicParsing="false" text="Red Blood Test" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <RadioButton mnemonicParsing="false" text="White Blood Test" wrapText="true" GridPane.columnIndex="2" GridPane.hgrow="ALWAYS" GridPane.rowIndex="2" />
      <RadioButton mnemonicParsing="false" text="Liver Function Test" GridPane.columnIndex="3" GridPane.rowIndex="2" />
      <RadioButton mnemonicParsing="false" text="Kidney Function Test" GridPane.columnIndex="4" GridPane.rowIndex="2" />
      <RadioButton mnemonicParsing="false" text="Electrolyte Test" GridPane.columnIndex="5" GridPane.rowIndex="2" />
      <Label text="Radiology Labs:" GridPane.rowIndex="1" />
      <RadioButton mnemonicParsing="false" text="X-Ray" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <RadioButton mnemonicParsing="false" text="CT Scan" GridPane.columnIndex="2" GridPane.rowIndex="1" />
      <RadioButton mnemonicParsing="false" text="MRI" GridPane.columnIndex="3" GridPane.rowIndex="1" />
      <CheckBox mnemonicParsing="false" text="Urinary Test" GridPane.columnIndex="0" GridPane.rowIndex="4" />
        <CheckBox mnemonicParsing="false" text="Stool Test" GridPane.columnIndex="0" GridPane.rowIndex="5" />
      <Button alignment="CENTER" mnemonicParsing="false" text="Cancel" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowIndex="6" />
      <Button mnemonicParsing="false" text="Submit" GridPane.columnIndex="3" GridPane.halignment="CENTER" GridPane.rowIndex="6" />
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</GridPane>
